{
    "name": "popphp/pop-config",
    "description": "Pop Config Component for Pop PHP Framework",
    "keywords": [
        "config",
        "configuration",
        "app config",
        "application configuration",
        "php",
        "pop",
        "pop php"
    ],
    "homepage": "http://www.popphp.org/",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nick Sagona",
            "email": "dev@nolainteractive.com",
            "homepage": "http://www.nolainteractive.com/"
        }
    ],
    "require": {
        "php": ">=7.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0.0"
    },
    "autoload": {
        "psr-4": {
            "Pop\\Config\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pop\\Config\\Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.2.x-dev"
        }
    }
}

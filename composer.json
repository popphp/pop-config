{
    "name": "popphp/pop-config",
    "description": "Pop Config Component for Pop PHP Framework",
    "keywords": [
        "config",
        "configuration",
        "app config",
        "application configuration",
        "php",
        "pop",
        "pop php"
    ],
    "homepage": "http://www.popphp.org/",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nick Sagona",
            "email": "dev@nolainteractive.com",
            "homepage": "http://www.nolainteractive.com/"
        }
    ],
    "require": {
        "php": ">=8.1.0",
        "popphp/pop-utils": "^2.0.0",
        "ext-json": "*",
        "ext-simplexml": "*",
        "ext-yaml": "*",
        "ext-dom": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0.0"
    },
    "autoload": {
        "psr-4": {
            "Pop\\Config\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pop\\Config\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        }
    }
}
